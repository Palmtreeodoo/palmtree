<odoo>
    <data>
        <report
                id="report_delivery_note"
                model="stock.picking"
                string="Delivery Print"
                name="delivery_note_print.delivery_note"
                file="delivery_note_print.delivery_note"
                report_type="qweb-pdf"/>
        <template id="delivery_note">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <!--<t t-call="report.external_layout">-->
                    <div class="page">
                        <div class="row">
                            <div class="col-xs-12"
                                 style="text-align: center;font-size:20px;font-family:Calibri !important;">
                                <br/>
                                <br/>
                                <b>DELIVERY NOTE</b>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-12">
                                <![CDATA[&nbsp;]]>
                            </div>
                        </div>

                        <div class="row">
                            <center>
                                <table border="1"  style="width:97%;border-collapse:collapse;">
                                    <tr>
                                        <td rowspan="4" style="text-align: left;border: 1px solid black;padding:5px;">
                                            <b>
                                                <sapn t-field="doc.location_id.operating_unit_id.partner_id.name"/>
                                            </b>
                                            <br/>
                                            <sapn t-field="doc.location_id.operating_unit_id.partner_id.street"/>
                                            <br/>
                                            <sapn t-field="doc.location_id.operating_unit_id.partner_id.street2"/>,PIN-
                                            <sapn t-field="doc.location_id.operating_unit_id.partner_id.zip"/>
                                            <br/>
                                            <sapn t-field="doc.location_id.operating_unit_id.partner_id.city"/>
                                            <sapn t-field="doc.location_id.operating_unit_id.partner_id.website"/>
                                            <br/>
                                            Mob :
                                            <sapn t-field="doc.location_id.operating_unit_id.partner_id.phone"/>
                                            <br/>
                                            GSTIN/UIN :
                                            <sapn t-field="doc.location_id.operating_unit_id.partner_id.vat"/>
                                            <br/>
                                            E-mail:
                                            <sapn t-field="doc.location_id.operating_unit_id.partner_id.email"/>
                                            <br/>
                                        </td>
                                        <td style="text-align: left;border: 1px solid black;padding:5px;">
                                            Delivery Note No.
                                            <span t-field="doc.name"/>
                                        </td>
                                        <td style="text-align: left;border: 1px solid black;padding:5px;">
                                            Dated
                                            <span t-field="doc.scheduled_date" t-options='{"widget": "date"}'/>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td style="text-align: left;border: 1px solid black;padding:5px;" >

                                        </td>
                                        <td style="text-align: left;border: 1px solid black;padding:5px;" >
                                            Mode/Terms of Payment
                                        </td>

                                    </tr>
                                    <tr>
                                        <td style="text-align: left;border: 1px solid black;padding:5px;" >
                                            Supplier's Ref.
                                        </td>
                                        <td style="text-align: left;border: 1px solid black;padding:5px;" >
                                            Other Reference(s)
                                        </td>
                                    </tr>
                                    <tr>
                                        <td style="text-align: left;border: 1px solid black;padding:5px;" >
                                            Buyer's Order No.
                                        </td>
                                        <td style="text-align: left;border: 1px solid black;padding:5px;" >
                                            Dated
                                        </td>
                                    </tr>
                                    <tr>
                                        <td rowspan="3" style="text-align: left;border: 1px solid black;padding:5px;" >

                                            Buyer
                                            <br/>
                                            <t t-if="doc.partner_id">
                                                <b>
                                                <sapn t-field="doc.partner_id.name"/>
                                                </b>
                                                <br/>
                                                <sapn t-field="doc.partner_id.street"/>
                                                <br/>
                                                <sapn t-field="doc.partner_id.street2"/>,PIN-
                                                <sapn t-field="doc.partner_id.zip"/>
                                                <br/>
                                                <sapn t-field="doc.partner_id.city"/>
                                                <sapn t-field="doc.partner_id.website"/>
                                                <br/>
                                                Mob :
                                                <sapn t-field="doc.partner_id.phone"/>
                                                <br/>
                                                GSTIN/UIN :
                                                <sapn t-field="doc.partner_id.vat"/>
                                                <br/>
                                                State Name :
                                                <sapn t-field="doc.partner_id.state_id.name"/>
                                                <br/>

                                            </t>
                                            <t t-if="not doc.partner_id">
                                                <b>
                                                    <sapn t-field="doc.location_dest_id.operating_unit_id.partner_id.name"/>
                                                </b>
                                                <br/>
                                                <sapn t-field="doc.location_dest_id.operating_unit_id.partner_id.street"/>
                                                <br/>
                                                <sapn t-field="doc.location_dest_id.operating_unit_id.partner_id.street2"/>-PIN-
                                                <sapn t-field="doc.location_dest_id.operating_unit_id.partner_id.zip"/>
                                                <br/>
                                                <sapn t-field="doc.location_dest_id.operating_unit_id.partner_id.city"/>
                                                <sapn t-field="doc.location_dest_id.operating_unit_id.partner_id.website"/>
                                                <br/>
                                                Mob :
                                                <sapn t-field="doc.location_dest_id.operating_unit_id.partner_id.phone"/>
                                                <br/>
                                                GSTIN/UIN :
                                                <sapn t-field="doc.location_dest_id.operating_unit_id.partner_id.vat"/>
                                                <br/>
                                                State Name :
                                                <sapn t-field="doc.location_dest_id.operating_unit_id.partner_id.state_id.name"/>
                                                <br/>
                                            </t>


                                        </td>
                                        <td style="text-align: left;border: 1px solid black;padding:5px;">
                                            Despatch Document No
                                        </td>
                                        <td style="text-align: left;border: 1px solid black;padding:5px;" >
                                        </td>
                                    </tr>
                                    <tr>
                                        <td style="text-align: left;border: 1px solid black;padding:5px;" >
                                            Despatched through
                                        </td>
                                        <td style="text-align: left;border: 1px solid black;padding:5px;" >
                                            Destination
                                        </td>

                                    </tr>
                                    <tr>
                                        <td colspan="2" style="text-align: left;border: 1px solid black;padding:5px;" >
                                            Terms Of Delivery
                                        </td>
                                    </tr>

                                </table>
                            </center>
                        </div>
                        <div class="row">
                            <div class="col-xs-12">
                                <![CDATA[&nbsp;]]>
                            </div>
                        </div>

                        <div class="row">
                            <center>
                                <table border="1" style="width:97%;border-collapse:collapse;">
                                    <thead>
                                        <tr style="background-color:#2d405f;color:white;">
                                            <th style="border: 1px solid black;padding:5px;">
                                                <center>Sl No</center>
                                            </th>
                                            <th style="border: 1px solid black;padding:5px;">
                                                <center>Discription Of Goods</center>
                                            </th>
                                            <th style="border: 1px solid black;padding:5px;">
                                                <center>HSN/SAC</center>
                                            </th>

                                            <th style="border: 1px solid black;padding:5px;">
                                                <center>GST Rate</center>
                                            </th>

                                            <th style="border: 1px solid black;padding:5px;">
                                                <center>Quantity</center>
                                            </th>

                                            <th style="border: 1px solid black;padding:5px;">
                                                <center>Rate</center>
                                            </th>

                                            <th style="border: 1px solid black;padding:5px;">
                                                <center>Per</center>
                                            </th>
                                            <th style="border: 1px solid black;padding:5px;">
                                                <center>Disc %</center>
                                            </th>
                                            <th style="border: 1px solid black;padding:5px;">
                                                <center>Amount</center>
                                            </th>

                                        </tr>

                                    </thead>
                                    <tbody class="purchase_tbody">
                                        <t t-set="i" t-value="1"/>
                                        <t t-set="f" t-value="0"/>
                                        <t t-foreach="doc.move_line_ids_without_package" t-as="l">
                                            <tr t-if="l.product_id">
                                                <td style="border: 1px solid black;text-align:center;padding:5px;">
                                                    <span t-esc="i"/>
                                                    <t t-set="i" t-value="i+1"/>
                                                    <!--<span t-field="l.sequence_number"/>-->
                                                </td>
                                                <td style="border: 1px solid black;padding:5px;">
                                                    <span t-field="l.product_id"/>
                                                </td>
                                                <td style="border: 1px solid black;padding:5px;">
                                                    <span t-field="l.product_id.l10n_in_hsn_code"/>
                                                </td>
                                                <td style="border: 1px solid black;padding:5px;">
                                                    <t t-foreach="l.taxes_id" t-as="c">
                                                        <span t-field="c.name"/>
                                                    </t>
                                                </td>
                                                <td style="border: 1px solid black;padding:5px;">
                                                    <span t-field="l.qty_done"/>
                                                </td>
                                                <td style="border: 1px solid black;text-align:right;padding:5px;">
                                                    <span t-field="l.price_unit"/>
                                                </td>
                                                <td style="border: 1px solid black;text-align:center;padding:5px;">
                                                    <span t-field="l.product_uom_id"/>
                                                </td>
                                                <td style="border: 1px solid black;text-align:center;padding:5px;">
                                                    <!--<span t-field="l.product_uom_id"/>-->
                                                </td>
                                                <td style="border: 1px solid black;text-align:right;padding:5px;">
                                                    <span t-esc="'{0:,.2f}'.format(float(l.price_total))"/>
                                                    <t t-set="f" t-value="f +(l.qty_done * l.price_unit)"/>
                                                    <!--f=l.qty_done * l.product_id.lst_price-->
                                                </td>
                                            </tr>
                                        </t>


                                        <tr>
                                            <td colspan="8" style="text-align:center;border: 1px solid black;padding:5px;">
                                                <b>
                                                    <i>Total</i>
                                                </b>
                                            </td>
                                            <td style="text-align:right;border: 1px solid black;padding:5px;">
                                                <span t-esc="'{0:,.2f}'.format(float(doc.amount_total))"/>
                                            </td>
                                        </tr>

                                        <tr>
                                            <td colspan="8" style="text-align:left;border: 1px solid black;padding:5px;">
                                                <span t-esc="doc.currency_id.amount_to_text(doc.amount_total)"/>
                                                <![CDATA[&nbsp;]]>Only
                                            </td>
                                            <td style="text-align:right;border: 1px solid black;padding:5px;">
                                                <b>
                                                    <i>E.&amp;O.E</i>
                                                </b>
                                            </td>

                                        </tr>

                                        <tr>
                                            <td colspan="4" style="text-align:left;border: 1px solid black;padding:5px;">
                                                <b>
                                                    <i>Recd.in Good Condition</i>
                                                </b>
                                            </td>

                                            <td colspan="5" style="border: 1px solid black;text-align:right;padding:5px;">
                                                <b>For PALM TREE</b>
                                                <br/>
                                                <br/>
                                                Authorised Signatory
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>

                            </center>
                        </div>
                    </div>
                </t>
            </t>
        </template>
    </data>
</odoo>